
public class Exer5 {
	
	public static void main(String[] args){
		boolean var1;
		var1 = true;
		System.out.println(var1);
		var1 = false;
		System.out.println(var1);
		
		byte v3 = 10;
		System.out.println(v3);
		v3 = 100; 
		System.out.println(v3);
		
		short v4 = 200;
		System.out.println(v4);
		
		int v5 = 10000;
		System.out.println(v5);
		
		float v6 = 10.40f;
		System.out.println(v6);
	}
}
