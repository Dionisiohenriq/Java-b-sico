
public class Exer16 {

	public static void main(String[] args) {
		int v1 = 10;
		if (v1 > 5) {
			System.out.println("é maior");
			System.out.println("bloco1");
		} else {
			System.out.println("é menor");
			System.out.println("bloco2");
		}
		System.out.println("o programa continua");
		if (v1 > 20) {
			System.out.println("é maior");
			System.out.println("bloco1");
		} else {
			System.out.println("é menor");
			System.out.println("bloco2");
		}
		System.out.println("o programa continua");
	}

}
